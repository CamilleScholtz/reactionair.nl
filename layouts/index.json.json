{{- $articles := where site.RegularPages "Type" "eq" "artikelen" -}}
{{- $.Scratch.Add "index" slice -}}
{{- range $articles }}
	{{- $.Scratch.Add "index" (dict
		"title" .Params.title
        "subtitle" .Params.subtitle
        "authors" .Params.auteurs
        "content" .Plain
        "permalink" .Permalink
    ) -}}
{{- end -}}
{{- $.Scratch.Get "index" | jsonify -}}
